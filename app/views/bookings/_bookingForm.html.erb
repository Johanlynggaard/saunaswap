<div id="sauna-booking-dates" class="booking-container" data-unavailable="<%= @sauna.unavailable_dates.to_json %>">
  <%= simple_form_for [@sauna , @booking] do |f| %>
    <%= f.input :start_date, as: :string, input_html: {class: "datepicker"} %>
    <%= f.input :end_date, as: :string, input_html: {class: "datepicker"} %>
    <%= f.submit class: 'btn btn-primary mb-3 btn-block' %>
  <% end %>
  <%= link_to 'Back to Sauna page', saunas_path, class: 'btn btn-warning btn-block' %>
</div>
